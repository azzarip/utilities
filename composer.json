{
    "name": "azzarip/utilities",
    "description": "Utilities for websites using Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paride Azzari",
            "email": "paride21azzari@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^12.0||^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "livewire/livewire": "^3.5"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "laravel/sanctum": "^4.0",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9||^10",
        "pestphp/pest": "^4",
        "pestphp/pest-plugin-arch": "^4",
        "pestphp/pest-plugin-laravel": "^4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Azzarip\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Azzarip\\AdminPanel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "App\\Http\\": "tests/Classes/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Azzarip\\Utilities\\AzzaripServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}